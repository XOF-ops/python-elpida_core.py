{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "ElpidaS3ReadWrite",
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:DeleteObject",
                "s3:ListBucket",
                "s3:GetBucketVersioning",
                "s3:ListBucketVersions",
                "s3:GetObjectVersion"
            ],
            "Resource": [
                "arn:aws:s3:::elpida-consciousness",
                "arn:aws:s3:::elpida-consciousness/*"
            ]
        },
        {
            "Sid": "ElpidaBucketSetup",
            "Effect": "Allow",
            "Action": [
                "s3:CreateBucket",
                "s3:PutBucketVersioning",
                "s3:PutEncryptionConfiguration",
                "s3:PutLifecycleConfiguration",
                "s3:PutBucketPublicAccessBlock",
                "s3:GetBucketVersioning",
                "s3:GetEncryptionConfiguration"
            ],
            "Resource": "arn:aws:s3:::elpida-consciousness"
        }
    ]
}
